\begin_deeper
\begin_layout Standard

\series bold
\size scriptsize
\color blue
for
\series default
 
\begin_inset Formula $i\in[m]$
\end_inset

 
\end_layout

\begin_layout Standard

\size scriptsize
\color blue
\begin_inset Formula $\quad$
\end_inset


\begin_inset Formula $X_{i}:=d_{i}-h_{i}^{T}\mathcal{F}$
\end_inset


\begin_inset Formula $\text{\qquad}$
\end_inset


\family typewriter

\backslash
* The inverse operator is denoted 
\begin_inset Formula $X_{i}^{-1}$
\end_inset

 *
\backslash

\end_layout

\begin_layout Standard

\series bold
\size scriptsize
\color blue
end for
\end_layout

\begin_layout Pause

\end_layout

\begin_layout Standard

\size scriptsize
\color brown
\begin_inset Formula $\quad$
\end_inset

Sort the elements of the 
\begin_inset Formula $\{X_{i}\}_{i=1}^{m}$
\end_inset

s with respect to cardinality of the elements in a descending order.
 
\end_layout

\begin_layout Standard

\size scriptsize
\color brown
\begin_inset Formula $\quad$
\end_inset

Denote the index set of the sorted set by 
\begin_inset Formula $\{s_{1},\ldots,s_{m}\}$
\end_inset

 such that 
\begin_inset Formula $|X|_{s_{1}}\geq\cdots\geq|X|_{s_{m}}.$
\end_inset


\end_layout

\begin_layout Pause

\end_layout

\begin_layout Standard

\series bold
\size scriptsize
\color teal
for 
\series default

\begin_inset Formula $i\in[m]$
\end_inset


\end_layout

\begin_layout Standard

\size scriptsize
\color teal
\begin_inset Formula $\quad$
\end_inset

 
\begin_inset Formula $X_{s_{i}}^{*}:=\text{argmin}_{x_{s_{i}}\in X_{s_{i}}}f_{s_{i}}(x_{s_{i}})$
\end_inset

 
\begin_inset Formula $\qquad$
\end_inset


\family typewriter

\backslash
* Univariate optimization problem *
\backslash

\end_layout

\begin_layout Standard

\size scriptsize
\color teal
\begin_inset Formula $\quad$
\end_inset


\begin_inset Formula $\mathcal{F}_{s_{i}}^{*}:=\bigcup_{x_{s_{i}}\in X_{s_{i}}^{*}}(X_{s_{i}}^{*})^{-1}(x_{s_{i}})$
\end_inset

 
\begin_inset Formula $\qquad$
\end_inset


\family typewriter

\backslash
* 
\series bold
Lemma.

\series default
 The set 
\begin_inset Formula $\mathcal{F}_{s_{i}}^{*}$
\end_inset

 is nonempty *
\backslash

\family default
 
\end_layout

\begin_layout Standard

\size scriptsize
\color teal
\begin_inset Formula $\quad$
\end_inset


\series bold
if 
\series default

\begin_inset Formula $i\leq m$
\end_inset


\end_layout

\begin_layout Standard

\size scriptsize
\color teal
\begin_inset Formula $\quad\quad$
\end_inset


\begin_inset Formula $X_{s_{i+1}}:=\left\{ d_{s_{i+1}}-h_{s_{i+1}}^{T}z\mid z\in\mbox{\mathcal{F}}_{s_{i}}^{*}\right\} .$
\end_inset


\end_layout

\begin_layout Standard

\size scriptsize
\color teal
\begin_inset Formula $\quad$
\end_inset


\series bold
end if
\end_layout

\begin_layout Standard

\series bold
\size scriptsize
\color teal
end for
\end_layout

\begin_layout Standard

\series bold
\size scriptsize
\color teal
return
\series default
 
\begin_inset Formula $\mathcal{F}_{s_{m}}^{*}$
\end_inset


\begin_inset Formula $\qquad$
\end_inset

 
\family typewriter

\backslash
* 
\series bold
Theorem.

\series default
 Any member of 
\begin_inset Formula $\mathcal{F}_{s_{m}}^{*}$
\end_inset

 is a Pareto optimal point *
\backslash

\end_layout

